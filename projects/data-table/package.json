{
  "name": "@i-cell/data-table",
  "version": "1.0.0",
  "scripts": {
    "build": "ng build `basename $(pwd -P)`",
    "build:prod": "npm run build -- --prod",
    "publish:custom": "npm run build:prod && npm publish dist"
  },
  "description": "Data table component for Angular 9 projects @ i-Cell",
  "main": "index.ts",
  "contributors": [
    "Egry Csaba",
    "Nagy Gábor",
    "Németh Zsófia",
    "Onofer Dávid",
    "Szabó Nándor",
    "Szalai József"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/i-Cell-Mobilsoft-Open-Source/icell-data-table"
  },
  "peerDependencies": {
    "@angular/cdk": "^9.0.0",
    "@angular/core": "^9.0.0",
    "@angular/forms": "^9.0.0",
    "@angular/material": "^9.0.0",
    "@mdi/angular-material": "latest",
    "@ngx-translate/core": "^12.0.0",
    "angular-resizable-element": "latest",
    "lodash-es": "^4.17.10",
    "ng-dynamic-component": "latest",
    "ngx-webstorage": "^6.0.0",
    "rxjs": "^6.5.3"
  },
  "devDependencies": {},
  "ngPackage": {
    "$schema": "../../node_modules/ng-packagr/ng-package.schema.json",
    "dest": "../../dist",
    "lib": {
      "cssUrl": "none",
      "entryFile": "index.ts",
      "umdModuleIds": {
        "@icell/base": "icellBase",
        "@ngx-translate/core": "ngxTranslateCore",
        "lodash": "lodash",
        "lodash-es": "lodashEs",
        "ngx-webstorage": "ngxWebstorage",
        "angular-resizable-element": "angularResizableElement",
        "ng-dynamic-component": "ngDynamicComponent",
        "ngx-multi-line-ellipsis": "ngxMultiLineEllipsis"
      }
    },
    "whitelistedNonPeerDependencies": [
      "tslib"
    ]
  }
}
